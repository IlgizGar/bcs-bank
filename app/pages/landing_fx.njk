{% extends "layout/layout.njk" %}
{#{% extends "library/card/card.njk" %}#}

{% set root = "." %}
{% set title = "FX_landing" %}
{% set header = 'blue' %}

{% import "../components/library/heading/heading.njk" as Heading %}
{% import "../components/library/button/button.njk" as Button %}
{% import "../components/library/feature/feature.njk" as Feature %}
{% import "../components/modules/landing-feature-card/landing-feature-card.njk" as LandingFeature %}
{% import "../components/library/card/card.njk" as Card with context %}
{% import "../components/library/input/input.njk" as Input %}
{% import "../components/modules/brand-features/brand-features.njk" as BrandFeatures %}
{% import "../components/library/dropdown/dropdown.njk" as Dropdown %}
{% import "../components/library/radio/radio.njk" as Radio %}

{% block body %}
  <div class="page js-page">

    {% include "../components/modules/header/headerLanding.njk" %}

    <main role="main" class="products-detail landing-fx">
      <section class="section page__section section__landing-banner">
        <div class="section__header section__header_theme-blue">
          <div class="page__container">
            <div class="section__row">
              <div class="products-detail__content js-scroll-animate">
                <div class="title-head">
                  Дебетовая карта
                </div>
                {{ Heading.heading({ 'tag': 'h2', 'title': 'Лучшая карта для \n работы с валютой', 'mods': { 'type': 'large', 'margin': 'medium' } }) }}
                <div class="features">
                  {{ Feature.feature({ 'title': 'от 0 ₽ ', 'text': 'обслуживание', 'mods': {'type': 'normal', 'theme': 'light'}, 'class': 'features__item' }) }}
                  {{ Feature.feature({ 'title': 'до 4%', 'text': 'валютный вклад', 'mods': {'type': 'normal', 'theme': 'light'}, 'class': 'features__item' }) }}
                  {{ Feature.feature({ 'title': 'до 2%', 'text': 'вернем бонусами', 'mods': {'type': 'normal', 'theme': 'light'}, 'class': 'features__item' }) }}
                </div>
              </div>
              <div class="products-detail__figure js-scroll-animate" data-illustration="">
                <div class="illustration">
                  <img class="illustration-image illustration-image_landing-fx" src="assets/illustrations/land_fx_card_2.png" alt="БКС Platinum">
                  <img class="illustration-image illustration-image_landing-fx" src="assets/illustrations/land_fx_card_1.png" alt="БКС Platinum">
                </div>
              </div>
            </div>
          </div>
          <div class="section__nav-wrapper">
            <div class="section__nav section__nav_no-border products-detail__nav">
              <div class="page__container">
                <div class="section__wrap">
                  {{ Button.button({ 'title': 'Оформить карту', 'mods': { 'theme': 'white' }, 'class': 'products-detail__nav-button products-detail__nav-button-left', attr: {'scroll': '#section-request'} }) }}
                </div>
              </div>
            </div>
          </div>
        </div>
      </section>

      <section class="section section_landing_features page__section js-scroll-animate">
        <div class="page__container">
          {{ Heading.heading({ 'tag': 'h2', 'title': 'Преимущества валютной карты', 'mods': { 'type': 'default', 'margin': 'medium' }, 'class':'' }) }}

          <div class="js-carousel carousel__content" data-id="landing_features" data-breakpoint="925">
            {{ LandingFeature.landigFeatureCard({ 'title': 'Валютная карта ', 'img': 'assets/illustrations/land_fx_card_1.png', 'features':[
              { 'title': '0₽ ', 'text': 'бесплатное обслуживание при обороте в 40 000₽ или среднемесячном остатке в 40 000₽ на всех счетах', 'mods': {'type': 'normal', 'theme': 'light'}, 'class': 'features__item' },
              { 'title': '1% ', 'text': 'от суммы покупок вернем бонусами', 'mods': {'type': 'normal', 'theme': 'light'}, 'class': 'features__item' },
              { 'title': 'до 3,8% ', 'text': 'вклад в USD', 'mods': {'type': 'normal', 'theme': 'light'}, 'class': 'features__item' },
              { 'title': '0₽  ', 'text': 'бесплатное открытие и обслуживание счетов в 3-х валютах: Рубли, Доллары США, Евро', 'mods': {'type': 'normal', 'theme': 'light'}, 'class': 'features__item' },
              { 'title': 'до 150 000₽ ', 'text': 'бесплатное снятие наличных', 'mods': {'type': 'normal', 'theme': 'light'}, 'class': 'features__item' },
              { 'title': '₽, $, € ', 'text': 'льготный курс обмена валют', 'mods': {'type': 'normal', 'theme': 'light'}, 'class': 'features__item' }
