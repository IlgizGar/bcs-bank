            <div class="carousel-controls__pagination js-carousel-pagination" data-carousel="landing_features">
              <span>01</span><span></span><span></span>
            </div>
          </div>
        </div>
      </section>


      <section class="section page__section section_landing_services services js-scroll-animate">
        <div class="page__container">
          {{ Heading.heading({ 'tag': 'h2', 'title': 'Обмен валюты', 'mods': { 'type': 'default', 'margin': 'default' } }) }}
          <div class="landing-exchange">
            <div class="landing-exchange__header">
              <h3 class="landing-exchange__header-title">
                При покупке 2 000 USD <br> вы сэкономите
              </h3>
              <div class="landing-exchange__header-items">
                <div class="landing-exchange__header-item">
                  <img class="landing-exchange__header-item-img" src="assets/illustrations/land_fx_card_1.png">
                  <div class="landing-exchange__header-item-title">
                    до 2 500₽
                  </div>
                  <div class="landing-exchange__header-item-text">
                    по Валютной карте
                  </div>
                </div>

                <div class="landing-exchange__header-item">
                  <img class="landing-exchange__header-item-img" src="assets/illustrations/land_fx_card_2.png">
