                          <div class="calculator-hidden-out">
                                 <span
                                         data-calc=""
                                         data-params="valuteBye|select_bye_currency"
                                         data-bind=""
                                         data-ordered="2"
                                         data-round="2"
                                 ></span>
                          </div>


                          <div class="calculator-hidden-out">
                                 <span
                                         data-calc=""
                                         data-params="valuteBye|select_bye_currency,card|select_calc_select_card_exchange,valuteSell|select_sell_currency"
                                         data-bind="calc_exchange_curse"
                                         data-ordered="3"
