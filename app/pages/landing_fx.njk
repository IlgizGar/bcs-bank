                {{ Card.card({ 'exchange': 'true', 'type':'div', 'title':'для Валютных карт','subtitle':'', 'view': 'lowercase', 'theme': 'black', 'hoverType': 'button', 'class': 'section__element default-exchange-card js-valute-card', 'attr': {'api-url': 'https://bcs-bank.com/get_courses_update', 'key':'online_courses'}  }) }}
                {{ Card.card({ 'exchange': 'true', 'type':'div', 'title':'для Валютных карт Premium','subtitle':'', 'view': 'lowercase', 'theme': 'black', 'hoverType': 'button', 'class': 'section__element default-exchange-card js-valute-card-premium', 'attr': {'api-url': 'https://bcs-bank.com/get_courses_update', 'key':'online_courses'}  }) }}
              </div>
