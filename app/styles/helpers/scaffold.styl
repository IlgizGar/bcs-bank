*
  box-sizing border-box

a, button
  padding: 0
  border: none
  background: none
  outline: none
  text-decoration: none

h1, h2, h3, h4, h5, p, ul
  margin: 0
  padding: 0
  list-style: none

.state_hidden
  display: none !important

.state_invisible
  opacity: 0 !important
  visibility: hidden !important
  height: 0 !important
