.feature

  position: relative
  z-index: 250
  
  .feature__content
    .feature__title. .feature__text
      color: $color_elem

  svg
    @media (min-width: 768px)
      height: 17px
      width: 16px
      transform: translateX(-7px)

    @media (max-width: 767px)
      height: 12px
      width: 11px
      transform: translateX(-4px)
