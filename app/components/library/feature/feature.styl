.feature

  position: relative
  z-index: 250

  .feature__content
    .feature__title. .feature__text
      color: $color_elem

  svg
    @media (min-width: 1216px)
      height: 21px
      width: 16px
      transform: translateX(-7px)

    @media (max-width: 1215px)
      height: 11px
      width: 11px
      transform: translateX(-4px)
