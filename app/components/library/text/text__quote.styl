.text .text__quote
  position: relative
  margin-bottom: 56px
  padding-left: 48px
  &:before
    content: ''
    display: block
    height: 24px
    width: 24px
    position: absolute
    left: 0
    right: 0
    background-image: url("/assets/images/svg/quote.svg")
    background-repeat: no-repeat
    background-position: center center
