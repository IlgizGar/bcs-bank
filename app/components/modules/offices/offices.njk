    <div class="section__tabbar">
    {{ Tabbar.tabbar({ 'anchor': { 'theme': 'default-tab', 'view': 'underscore', 'line': 'blue', 'class': 'offices__tab-control'}, 'tabs': [{'title': 'Кассы', 'id': 'offices-tab' }, {'title': 'Банкоматы', 'id': 'atm-tab'}], 'class': 'offices__tabbar' }) }}
    </div>

    <div class="offices__search-wrapper">
      <div class="offices__search">
          {{ Input.input({ 'label': 'Введите адрес или метро', 'class': 'search__field-group', 'field':{'name': 'map-search'}, 'icon': 'search' }) }}
          {{ Button.button({ 'mods': {'type': 'icon','theme': 'transparent'}, 'attr': {'state': 'map'}, 'class': 'offices__switch-view js-offices-switcher' }) }}

        <div class="offices__search-variations"></div>
        {{SearchVariations.searchVariations ({'class': 'offices__switch-view js-search-variations' }) }}
      </div>
      <div class="offices__map-router js-map-router">
        <div class="offices__map-router-wrapper">
          <div class="offices__map-router--type offices__map-router--walk">
            <button class="offices__map-router-button js-map-router-button js-map-router--walk router-active" data-value="pedestrian">
              {{ Icon.icon({ 'name': 'icon_walk' }) }}
            </button>
            <div class="offices__map-router-time js-map-router-time"></div>
          </div>
          <div class="offices__map-router--type offices__map-router--bicycle">
            <button class="offices__map-router-button js-map-router-button js-map-router--bicycle" data-value="bicycle">
              {{ Icon.icon({ 'name': 'bicycle' }) }}
            </button>
            <div class="offices__map-router-time js-map-router-time"></div>
          </div>
          <div class="offices__map-router--type offices__map-router--car">
            <button class="offices__map-router-button js-map-router-button js-map-router--car" data-value="auto">
              {{ Icon.icon({ 'name': 'car' }) }}
            </button>
            <div class="offices__map-router-time js-map-router-time"></div>
          </div>
          <div class="offices__map-router--type offices__map-router--bus">
            <button class="offices__map-router-button js-map-router-button js-map-router--bus" data-value="masstransit">
              {{ Icon.icon({ 'name': 'bus' }) }}
            </button>
            <div class="offices__map-router-time js-map-router-time"></div>
          </div>
        </div>
        {{ Button.button({'title':'Отменить', 'mods': {'theme': 'default','view':'text'}, 'class': 'js-map-router-cancel' }) }}
      </div>
    </div>


    <div id="map-container-mobile" class="offices__map-wrapper offices__map-mobile" style="height: 678px;">
