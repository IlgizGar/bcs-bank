.menubar .menubar__logo
  height: 36px

  img
    height: 100%

    &:nth-child(1)
      display: none

    &:nth-child(2)
      display: block


  @media (max-width: 1215px)
    .menubar .menubar__logo
      height: 32px

  @media (max-width: 1023px)
    padding-top: 6px
    img
      height: 32px

  @media (min-width: 832px) and (max-width: 1279px)
    .menubar .menubar__logo
      height: 27px
