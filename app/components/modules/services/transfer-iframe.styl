.transfer-iframe
  width 100%
  height 520px
  border none
  position relative
  z-index 10
  margin-top: -100px
  @media (max-width 1279px)
    margin-top: -10px
  @media (max-width 815px)
    height 725px
    margin-top: -30px
  @media (max-width 592px)
    height 900px
    margin-top: -10px
    overflow hidden
