.transfer-iframe
  width 100%
  height 520px
  border none
  position relative
  z-index 10
  margin-top: -100px
  @media (max-width 1279px)
    margin-top: -10px
  @media (max-width 683px)
    height 725px
    margin-top: -30px
  @media (max-width 576px)
    height 735px
    margin-left -7px
    margin-right -7px
    margin-top: -10px
  overflow hidden
  @media (max-width 518px)
    height 825px
    margin-left -7px
    margin-right -7px
    margin-top: -10px
    overflow hidden
  @media (max-width 425px)
    height 900px
    margin-left -7px
    margin-right -7px
    margin-top: -10px
    overflow hidden
