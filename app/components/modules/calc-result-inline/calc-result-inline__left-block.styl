.calc-result-inline__left-block
  min-width 183px
  width 100%
  border-right 1px solid $color_borders
  padding-right 5px
  @media (max-width: 768px)
    border-right none
    &:after
      content ''
      display block
      width 60px
      margin 15px 0
      height 0
      border-bottom 1px solid $color_borders
